{
    "name": "freepeace/larabone",
    "type": "library",
    "description": "Personal laravel extension package for creating APIs",
    "keywords": ["larabone"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kin Basco",
            "email": "freepeace13@gmail.com",
            "homepage": "https://github.com/freepeace13"
        }
    ],
    "require": {
      "php": "^7.1.3",
      "illuminate/support": "5.8.*",
      "illuminate/database": "5.8.*"
    },
    "require-dev": {
      "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.8"
    },
    "autoload": {
      "psr-4": {
        "Freepeace\\Larabone\\": "src/"
      },
      "files": [
        "src/helpers.php"
      ]
    },
    "autoload-dev": {
      "psr-4": {
        "Freepeace\\Larabone\\Test\\": "tests"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Freepeace\\Larabone\\LaraboneServiceProvider"
        ]
      }
    }
}
